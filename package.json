{
  "name": "turbocat",
  "version": "1.0.0",
  "private": true,
  "description": "Turbocat - Multi-agent orchestration platform",
  "workspaces": [
    "turbocat-agent",
    "backend"
  ],
  "scripts": {
    "prepare": "husky",
    "dev:frontend": "npm run dev --workspace=turbocat-agent",
    "dev:backend": "npm run dev --workspace=backend",
    "build:frontend": "npm run build --workspace=turbocat-agent",
    "build:backend": "npm run build --workspace=backend",
    "test:frontend": "npm test --workspace=turbocat-agent",
    "test:backend": "npm test --workspace=backend",
    "lint:frontend": "npm run lint --workspace=turbocat-agent",
    "lint:backend": "npm run lint --workspace=backend",
    "typecheck:frontend": "npm run type-check --workspace=turbocat-agent",
    "typecheck:backend": "npm run typecheck --workspace=backend"
  },
  "devDependencies": {
    "husky": "^9.1.7"
  }
}
